{
    "manifest_version": 3,
    "name": "Text Manipulation Extension",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "scripting"
    ],  "commands": {
    "add_padding": {
        "suggested_key": {
            "default": "Alt+P"
        },
        "description": "Обернуть"
    }
},
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["jquery-3.6.0.min.js", "content.js"]
        }
    ]
}
